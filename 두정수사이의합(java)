class Solution {
    public long solution(int a, int b) {
        long answer = 0;
        int ai = 0;
        int bi = 0;
        if(a == b) return a;
        if(a > b) {
            ai = b;
            bi = a;
        } else {
          ai = a;
          bi = b;
            
        }
        answer = ai;
        for(int i = ai+1; i <= bi; i++) {
            answer += i;
        }
        return answer;
    }
}
