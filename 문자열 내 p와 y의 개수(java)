class Solution {
    boolean solution(String s) {
        boolean answer = false;
        int pNum = 0, yNum = 0;
        char ch = 0;
        for(int i = 0; i < s.length(); i ++) {
             ch = s.charAt(i);
            if(ch == 'p' || ch == 'P') pNum++;
            else if(ch == 'y' || ch == 'Y') yNum++;
        }
        if(pNum == yNum) answer = true;
        // [실행] 버튼을 누르면 출력 값을 볼 수 있습니다.
        System.out.println();

        return answer;
    }
}
